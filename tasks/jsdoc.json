{
  "options": {
    "template": "./node_modules/grunt-jsdoc/node_modules/ink-docstrap/template/",
    "configure": "./jsdoc.conf.json"
  },
  "main": {
    "src": ["README.md", "./build/docs/<%= pkg.main %>"],
    "dest": "./docs/",
    "jsdoc": "./node_modules/.bin/jsdoc",
    "options": {
      "private": false
    }
  },
  "dev": {
    "src": ["README.md", "./build/dev-docs/<%= pkg.main %>"],
    "jsdoc": "./node_modules/.bin/jsdoc",
    "dest": "./dev-docs/"
  }
}
